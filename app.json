{
  "expo": {
    "name": "iGrocery",
    "slug": "iGroceryApp",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios"],
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "description": "A lightweight grocery list app to track everything you need to pickup from the store.",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ajesamann.igrocery"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#002a52"
    }
  },
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "assetBundlePatterns": ["**/*"]
}
